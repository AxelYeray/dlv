inter(X) :- a(X), b(X).
union(X) :- a(X).
union(X) :- b(X).

dif(X) :- a(X), not inter(X).

% Propiedad reflexiva
reflexive(X) :- nodo(X), not rel(X, X).

dif_sim(X) :- union(X), not inter(X).

